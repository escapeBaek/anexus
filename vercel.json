{
  "builds": [
    {
      "src": "anhub/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "buildCommand": "bash setup.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "anhub/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ],
  "functions": {
    "api/*.py": {
      "maxDuration": 60
    }
  }
}
